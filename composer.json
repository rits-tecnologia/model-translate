{
  "name": "rits/model-translate",
  "description": "A Laravel package to translate models",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Lopes",
      "email": "danielorkae@outlook.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/translation": "^5.8|^6.0",
    "illuminate/support": "^5.8|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Rits\\ModelTranslation\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rits\\ModelTranslation\\Providers\\ServiceProvider"
      ]
    }
  }
}
